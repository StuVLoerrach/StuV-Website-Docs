  "variants": 9,
  "tags": ["hero", "landing", "header", "main"],
  "props": {
    "variant": {
      "type": "string",
      "required": true,
      "options": [
        "variant1",
        "variant2",
        "variant3",
        "variant4",
        "variant5",
        "variant6",
        "variant7",
        "variant8",
        "variant9"
      ],
      "description": "The visual variant of the hero section"
